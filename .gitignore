# gitignore template for Drupal 7 projects
#
# Ignore configuration files that may contain sensitive information.
sites/default/*settings*.php

# Ignore paths that contain generated content.
cache/
files/
sites/default/files
sites/*/private

#Ignore Contrib Modules
sites/all/modules/contrib

#Ignore Composer Vendor Folder
sites/all/modules/custom/user_sso_login/composer/vendor

# Ignore default text files
.htaccess
robots.txt
/CHANGELOG.txt
/COPYRIGHT.txt
/INSTALL*.txt
/LICENSE.txt
/MAINTAINERS.txt
/UPGRADE.txt
/README.txt
sites/all/README.txt
sites/all/modules/README.txt
sites/all/themes/README.txt

# Ignore everything but the "sites" folder ( for non core developer )
web.config
authorize.php
cron.php
index.php
install.php
update.php
xmlrpc.php
/script-txt
/includes
/misc
/modules
/profiles
/scripts
/themes
#Ignore ALL ZIP files
*.zip
#Ignore pg-SQL DB Backup Files
*.backup
#All Backup Files
*.*_bkp*
#Ignore ALL LOG files
*.log
*.log
